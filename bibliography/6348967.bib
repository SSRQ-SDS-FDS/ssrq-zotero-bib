@online{EpiCherchellTranscriptionCollaborative,
	title = {{EpiCherchell} - Transcription collaborative},
	url = {https://tacteo.huma-num.fr/project/33},
	abstract = {Le projet d’édition des Inscriptions antiques de Césarée de Maurétanie (Cherchell, Algérie) a été initié en 2010 par Philippe Leveau, professeur émérite d’archéologie à l’Université d’Aix-Marseille. Il a pour objectif de publier tous les textes épigraphiques, grecs et latins, de l’ancienne capitale de la province romaine de Maurétanie Césarienne. L’épigraphie de Césarée présente des inscriptions gravées entre l’époque des rois Juba {II} et Ptolémée et l’Antiquité tardive et chrétienne. Elle se distingue par sa richesse quantitative (quelque 1300 textes) et qualitative (avec des catégories d’inscriptions très diverses : dédicaces, hommages, épitaphes, etc. gravées sur des supports variés et parfois accompagnées de reliefs). Une base de données en ligne accompagne le projet de corpus en réunissant et mettant à disposition de la communauté scientifique les inscriptions étudiées et les photographies qui les illustrent. Les fiches actuellement disponibles ne présentent que les informations signalétiques sur l’objet traité et la transcription/traduction de l’inscription. Les analyses seront accessibles sur demande avec accord des auteurs. Utiliser la plateforme {TACTEO} nous permettrait de baliser le corps des inscriptions lapidaires et aider à l’encodage typographique, sémantique, analytique du support et du texte des inscriptions étudiées dans la base selon le schéma {TEI} {EpiDoc} , de façon intuitive et proche des habitudes de travail des épigraphistes non adepte de l'encodage. Cette utilisation pourrait également être utilisé dans le cadre d’{EpiHorrea} (Plateforme d’exploitation et de valorisation de données épigraphiques de l’Afrique du Nord antique et du bassin méditerranéen), dont la structure se base sur {EpiCherchell}, plateforme destinée à accueillir, exploiter et diffuser plusieurs collections épigraphiques.},
	titleaddon = {{TACTEO}},
	type = {Plateforme collaborative},
	urldate = {2025-09-24},
}

@online{EpicureiTranscriptionCollaborative,
	title = {Epicurei - Transcription collaborative},
	url = {https://tacteo.huma-num.fr/project/5},
	abstract = {Epicurei : index épistémologique et transcriptions enrichies de textes épicuriens

https://eman-archives.org/Epicurei/

Le site Epicurei présente un index des principales notions constitutives de l’épistémologie épicurienne, ainsi qu'un choix de textes de l'école d'Epicure. Les textes sont proposés sous deux formes :
- des reproductions d'éditions anciennes libres de droit
- des transcriptions de certaines de ces éditions, enrichies d’un apparat critique minimal constitué à partir d’éditions plus récentes, et d'une traduction française du texte grec.
Les transcriptions enrichies contiennent des liens vers l'index, de façon à proposer pour chaque notion son contexte d'élaboration et d'usage.

Vous trouverez plus d'informations sur les transcriptions dans l'onglet Principes éditoriaux.

L'accès direct aux transcriptions se fait à partir du menu ci-dessous, onglet Accéder aux textes : cliquez sur le titre de l'Item puis Voir la transcription de cet item.

L'accès aux éditions se fait à partir de l'onglet Parcourir les contenus. Les descriptions des contenus signalent ceux qui sont accompagnés d'une transcriptions.

Epicurei est un site élaboré dans le cadre et avec le soutien du projet {SPIDER}, pour plus de détails voir l'onglet Soutiens du projet.},
	titleaddon = {{TACTEO}},
	type = {Plateforme collaborative},
	urldate = {2025-09-24},
}

@online{EMSGTranscriptionCollaborative,
	title = {{eMSG} - Transcription collaborative},
	url = {https://tacteo.huma-num.fr/project/10},
	abstract = {Musici Scriptores Graeci en ligne Le projet {eMSG} consiste en l'édition numérique du livre Musici Scriptores Graeci de Carl Jan en 1895, qui donnait le texte de plusieurs traités musicaux en grec ancien de divers auteurs (Aristote, Euclide, Cléonide, Nicomaque, Bacchius l’Ancien, Gaudence, Alypius), et des premières partitions de musique grecque connues. Sa longue préface donne une présentation détaillée de plus de 200 manuscrits qui conservent ces textes. Le premier objectif du projet est de lier ces descriptions à la base Pinakes de l'{IRHT} pour améliorer leur référencement. Un autre objectif est de référencer les termes techniques musicaux relevés dans les traités, d'en donner des définitions les plus exactes possibles et, le cas échéant, de donner une bibliographie des publications où ils sont commentés et étudiés. Ce projet formerait alors le cœur d'un premier dictionnaire des termes techniques musicaux. L'enrichissement des données a vocation à devenir collaboratif.},
	titleaddon = {{TACTEO}},
	type = {Plateforme collaborative},
	urldate = {2025-09-24},
}

