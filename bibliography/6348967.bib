@online{TranscriptionCollaborativeDarchives,
	title = {Transcription collaborative d'archives des collections des musées d’arts populaires},
	url = {https://tacteo.huma-num.fr/project/30},
	abstract = {Le « fichier X » décrit un large éventail d’objets témoins de savoir-faire et d’usages se rapportant à la vie rurale des régions françaises. Sur chaque fiche, sont associés un objet, une photographie et un ou plusieurs dessins techniques, une dizaine de critères descriptifs allant de la simple dénomination (mais y compris en langue vernaculaire) à des informations sur l’utilisation concrète des objets. Ce fichier a été constitué pour la plus grande part entre 1943 et 1946, alimenté jusqu’en 1964, dans le cadre d’un « chantier intellectuel » initié par le {MNATP} (ancêtre du Mucem actuel). Il recense et décrit des objets d’art populaire et d’ethnologie française dans les musées locaux à travers le pays alors que nombre de musées et leurs collections ont connu différents destins pendant et après la seconde guerre mondiale. La numérisation puis la transcription du « ficher X » va permettre de tirer parti d’une matière documentaire de grande qualité et encore inexploitée. L’objectif de ce travail est, dans un premier temps la validation du contenu de ces fiches qui, manuscrites, ne sont pas toujours parfaitement lisibles, avec potentiellement des fautes d’orthographe ou de syntaxe. Il s’agit de redonner à ces fiches une structure documentaire cohérente pour, dans un second temps, les analyser afin d’identifier à l’échelle nationale des corpus d’objets en fonction de critères thématiques, géographiques, techniques, en mettant ainsi en évidence des aires culturelles associant la démarche ethnologique à celle de l’archéologie. C’est tout un pan de la culture matérielle de l’ethnologie française constituée par les musées au début du {XXe} siècle qu’il s’agit de révéler tant aux professionnels des musées qu’au grand public curieux de patrimoine.},
	titleaddon = {{TACTEO}},
	type = {Plateforme collaborative},
	urldate = {2025-09-24},
}

@online{TheatreSophistesTranscription,
	title = {Théâtre des sophistes - Transcription collaborative},
	url = {https://tacteo.huma-num.fr/project/27},
	abstract = {Le Theatrum veterum rhetorum est un traité néo-latin sur les sophistes de l’Antiquité écrit par le jésuite Louis de Cressolles et publié en 1620 à Paris. Le terme de « théâtre » renvoie au genre savant, le theatrum, héritier du « miroir » médiéval (speculum), caractérisé par son contenu encyclopédique et ses visées didactique et morale. De plus, theatrum réfère aussi bien à l’aspect théâtral de la déclamation sophistique qu’à la manière même de Cressolles de mettre en scène, de donner à voir et à entendre les grands orateurs qu’étaient les sophistes autrefois. Le projet intitulé « Le théâtre des sophistes de l’Antiquité » a pour but d’éditer numériquement les transcriptions et traductions d’une partie du livre iii de l’ouvrage, support textuel d’un travail de thèse. Les chapitres retenus portent en particulier sur les sophistes de l’ère impériale romaine qui ont vécu entre le ier le ive siècle de notre ère, connus notamment à travers les œuvres prosopographiques de Philostrate (iiie s. ap. J.-C.) et d’Eunape (ive s. ap. J.-C.).},
	titleaddon = {{TACTEO}},
	type = {Plateforme collaborative},
	urldate = {2025-09-24},
}

