@article{witzInteroperabiliteBasesDonnees2025,
	title = {Interopérabilité des bases de données scientifiques : une sobriété de façade},
	volume = {11},
	rights = {https://creativecommons.org/licenses/by/4.0/},
	issn = {2736-2337},
	url = {https://journals.openedition.org/revuehn/4575},
	doi = {10.4000/1498w},
	shorttitle = {Interopérabilité des bases de données scientifiques},
	abstract = {Une tâche est plus sobre si elle consomme moins de ressources, sur l’ensemble de son cycle de vie. Dans le domaine des humanités numériques, certaines des tâches les plus difficiles ont trait à l’interopérabilité des bases de données, telles que la documentation, l’alignement, la migration ou la publication. Nécessitant souvent un savoir spécialisé, ces tâches peuvent vite devenir chronophages. Après avoir rappelé le contexte dans lequel ces tâches sont généralement réalisées, cet article proposera des éléments de solution pour réduire leur consommation en ressources, en particulier en ressources humaines. Enfin, il discutera les bénéfices et les limites de ces solutions.
Any task is more sustainable if it consumes fewer resources, over its entire lifecycle. Some of the challenges of Digital Humanities are tasks related to interoperability between databases, such as documentation, alignment, migration and publication. Often requiring specialised knowledge, these tasks can quickly become time-consuming. After recalling the context in which these tasks are generally undertaken, this article will propose solutions for reducing their consumption of resources, of human resources in particular. Finally, it will discuss the benefits and limitations of these solutions.},
	journaltitle = {Humanités numériques},
	shortjournal = {revuehn},
	author = {Witz, Régis and Porte, Guillaume},
	urldate = {2026-01-30},
	date = {2025},
}

