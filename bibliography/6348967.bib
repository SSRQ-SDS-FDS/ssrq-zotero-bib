@online{HoFraMeTranscriptionCollaborative,
	title = {{HoFraMe} - Transcription collaborative},
	url = {https://tact.demarre-shs.fr/project/24},
	abstract = {Participez à la transcription des archives hospitalières dans la France médiévale !

Vous pouvez à votre choix transcrire ou relire des pages sur cette plateforme, guidé pas à pas grâce à une interface qui se veut conviviale, en vous aidant du manuel du contributeur et du menu "aide" quand vous serez sur une page. Une fois inscrit, vous pourrez accéder à différents dossiers regroupant respectivement les pages par département.

Vous aurez alors l'occasion de découvrir ou redécouvrir des actes de fondation, des livres terriers, des actes de donations, des inventaires,..., Les fonds archivistiques ont toujours été traités, dépouillés, transcrits et interprétés de manière éparse et isolé. Il est donc temps de centraliser toutes ces données afin d'en constituer le corpus le plus exhaustif de France !

Ce projet est issu de la collaboration entre:

- L'Equipe {ELAN} (Equipe Littératures et Arts Numériques, {CNRS}-Université Grenoble-Alpes), qui travaille sur les humanités numériques. Elle développe notamment des outils et méthodes pour rendre accessibles et exploitables des fonds manuscrits inédits. {ELAN} et le programme Démarre {SHS} ! ont ainsi développé une plateforme de transcription contributive, {TACT}, qui permet la transcription et la modélisation des documents numérisés (pour plus d'informations: https://tact.demarre-shs.fr/)

- le laboratoire {CAHMER} (Centre d'Archéologie et d'Histoire Médiévales des Etablissements Religieux) de l'université de Picardie. Ainsi, son axe d'études hospitalières, piloté par Bruno Tabuteau, s'est aussi doté d'une base de données accessible et en open-data: {HOFRAME}

Votre participation est précieuse et c'est pour vous l'occasion d'une expérience inédite et d'une plongée dans l'univers fascinant des archives hospitalières : n'hésitez pas à vous lancer dans l'aventure...},
	titleaddon = {{TACT}},
	type = {Plateforme collaborative},
	urldate = {2025-09-24},
}

@online{GeohistoireSystemeAdministratif,
	title = {Géohistoire du système administratif des tusi dans la région des monts Wuling (Chine impériale tardive) - Transcription collaborative},
	url = {https://tacteo.huma-num.fr/project/32},
	abstract = {Le système des tusi, ou fonctionnaires natifs, est un régime particulier de l’administration impériale chinoise permettant le contrôle indirect des régions frontalières extérieures et intérieures de l’empire. Dans la Chine impériale tardive ({XIIIe}-{XIXe} siècles), il a été largement implanté dans la région des monts Wuling, dans la partie méridionale du cours moyen du Yangzi. À partir d’une approche géohistorique, cette étude se consacrera sur les relations des différents tusi avec les autorités centrales et les populations locales qu’ils dominent, mais également sur les interactions entre les différents territoires tusi de cette région. En combinant une étude des évolutions dans le temps et une approche spatiale, il s’agira de déterminer comment les liens entre les représentations de cet espace montagnard et les stratégies opérées par les différents acteurs sur place déterminent la façon dont le territoire se structure à différentes échelles. Dans un contexte d’expansion territoriale, les représentations spatiales et les savoirs géographiques détenus par l’empire sont un outil de domination de ces espaces. D’un autre côté, les tusi et populations locales disposent de leurs propres savoirs sur ces lieux qu’ils habitent. Les autorités impériales peuvent porter un intérêt pour ces savoirs vernaculaires, tout en véhiculant leurs propres représentations de ces espaces de montagnes. Cette tension entre ces types de représentation et les intérêts des différents acteurs porte des conséquences sur la manière dont le territoire est organisé. Cette étude proposera une analyse spatiale de la région, combinée à une étude des représentations et des savoirs spatiaux. Elle reposera sur l’étude de matériaux divers, allant des documents historiques écrits (codes impériaux, descriptions géographiques, généalogies des tusi, etc.) aux données issues des études archéologiques sur la région (épigraphie, rapports de fouille, données sur le bâti, les infrastructures routières, etc.).},
	titleaddon = {{TACTEO}},
	type = {Plateforme collaborative},
	urldate = {2025-09-24},
}

