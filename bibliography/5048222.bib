@article{Pupikofer:1869,
	title = {Geschichte der Freiherren von Klingen zu Altenklingen, Klingnau und Hohenklingen},
	volume = {10},
	shorttitle = {Klingen},
	abstract = {Sehr veraltete Darstellung. Kaum Quellen- oder Literaturbelege. Deutung des Namens 'Klinge' auf topographische Gegebenheiten: 'Klinge heißt im schwäbischen Landesdialekt ein Waldbach und in abgeleiteter Bedeutung eine Waldschlucht. Die Burg Klingen oder, wie sie erst später zum Unterschiede von Hohenklingen genannt wurde, Altenklingen liegt auf dem Scheitel von zwei solchen zusammenstoßenden Waldschluchten auf fester Molasse und Nagelfluh, eignete sich also ganz vorzüglich zu einer Burgfeste, wie die fehdenreiche Zeit des {XII}. Jahrhunderts es erforderte.' (S. 7). Danach folgen einige phantasievolle Ausschnitte und Paraphrasierungen aus der Zimmer'schen Chronik. Die von Pupikofer als 'wenigstens der Hauptsache nach wahr' angesehen werden. (S. 8f., hier 9). Die übergabe von Feldbach an die Schwestern in ponte könnte man dies als Neutralisierung umstrittenen Besitzes zwischen den beiden Linien und allenfalls anderen Herren deuten. Auch spätere Stiftungen sind etwas anrüchig, wie dieser dreigeteilte Hof in {TUB}.? Kann man das mit Sablonier diskutieren?},
