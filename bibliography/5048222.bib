@unpublished{cicchini_regulation_2025,
	location = {Lucerne},
	title = {La régulation de la nuit à l’épreuve de la nocturnalisation (Genève, Berne et Zurich 1500-1800)},
	abstract = {Dans un article inaugural publié en 2007, l’historien Craig Koslofsky propose le néologisme nocturnalization pour caractériser l’évolution du rapport aux heures noires durant l’époque moderne. Ce concept renvoie à l’expansion des usages légitimes – et visibles – de la nuit à partir de 1650. Dépassant les expressions négatives de l’obscurité forgées durant les siècles précédents, une nouvelle image et de nouveaux usages émergeraient durant le {XVIIe} siècle. Dans les cours et les grandes villes européennes, la vie sociale nocturne ne serait plus interdite et réprimée, ou seulement tolérée par les élites, mais désormais valorisée.

À l’appui de la riche collection des Sources du droit suisse, cette contribution interroge le processus de nocturnalisation en Suisse entre 1600 et 1800. Le propos s’appuie sur un corpus d’une centaine de règles de droit édictées durant cette période dans plusieurs villes suisses, principalement Berne, Genève, Saint-Gall et Zürich.},
	note = {La nuit est encore jeune. Sociabilités nocturnes et régulations sociales à l'époque moderne},
	author = {Cicchini, Marco},
	date = {2025-09-07},
	langid = {french},
}

