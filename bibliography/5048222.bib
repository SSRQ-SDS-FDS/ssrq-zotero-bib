@unpublished{dorthe_go_2025,
	location = {Leiden},
	title = {Go away or burn! Banishment of witches in Fribourg (Switzerland): 15th-18th century},
	url = {https://event.esshc2025.exordo.com/presentation/832/go-away-or-burn-banishment-of-witches-in-fribourg-switzerland-15th-18th-century},
	abstract = {Historiography devoted to the study of banishment as a penal sanction has long emphasized its ignominious nature. Since then, several studies (particularly in Italy and Germany) have put this view into perspective by highlighting that the effects of banishment were often very individual and depended on numerous factors.

Is this also true in the context of witchcraft trials? When and why did the judges of Fribourg (Switzerland) pronounce a sentence of banishment, when normally the punishment of burning at the stake was promised to witches?

The study of witchcraft trials conducted by the City Court of Fribourg between the 15th and 18th centuries provides answers to these questions and reveals that very often, in the absence of a confession, the judges pronounced a sentence of banishment.

In this context, there is also the question of the illegal return of the banished to the lands from which they were expelled. The practice of punitive exile thus reveals the great leniency of the authorities towards banished persons who returned clandestinely. Contrary to legal requirements (Imperial law), the majority of them were not executed but only banished again.

Although the profile of those banished varied greatly, it seems clear that the judge's arbitrary power allowed for a great flexibility. In this way, if a person suspected of witchcraft did not confess to any crime related to witchcraft, the judge could still sentence them to exile, and thus exclude from society a person considered disruptive. Banishment thereofre made it possible to circumvent the absence of confession, which was normally mandatory for sentencing a witch to the stake.},
	note = {Outcasts and criminal justice},
	author = {Dorthe, Lionel},
	date = {2025-03-26},
}

